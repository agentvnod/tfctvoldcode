@model List<HomepageFeatureItem>
<ul>
    @{if (Model != null)
      {
          if (Model.Count() > 0)
          {
              foreach (var item in Model)
              {
        <li><a href="@Url.Action("TeamDetails", "UAAP", new { id = item.id, slug = item.slug })">@item.name</a></li>
              }
          }
      }
    }
</ul>
