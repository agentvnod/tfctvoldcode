@model List<HomepageFeatureItem>
@{if (Model != null)
  {
      if (Model.Count() > 0)
      {
          foreach (var item in Model)
          {
    <div class="movie col-md-3 col-sm-4 col-xs-6">
        <a href="@Url.Action("TeamDetails", "UAAP", new { id = item.id, slug = item.slug })">
            <img src="@item.imgurl" alt="@item.name"></a>
        <h3 class="small_header">
            <a href="@Url.Action("TeamDetails", "UAAP", new { id = item.id, slug = item.slug })">@item.name</a></h3>
        <span class="date">@item.description</span>
    </div>
          }
      }
  }
             
}
