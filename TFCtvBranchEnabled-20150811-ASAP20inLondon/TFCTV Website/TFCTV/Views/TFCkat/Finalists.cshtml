        //            }
        //            else {
        //                $('#latestShows_bodyContainer>div.showItem_preview').tsort({ attr: $(this).find("option:selected").val() });
        //            }
        //        });

        var sortopt = ["meast", "asia", "anz"];
        $('.sort-option').click(function () {
            var isFound = $.inArray(this.id, sortopt);
            if (isFound >= 0)
                $('.dTeamsList>div.regionList').tsort({ attr: this.id, order: $(this).attr('rel') });
            else
                $('.dTeamsList>div.regionList').tsort({ attr: this.id, order: $(this).attr('rel2') });
            return false;
        });
        $(document).ready(function () {
            $(' .TFCKatLoves, .TFCKatLovesNumber').live('click', function () {
                var epid = $(this).attr('rel');
                window.location.href = "/TFCkat/ContenderDetails/" + epid;
                return false;
            });
        });
    </script>
}
