@model IPTV2_Model.User
<div id="UserActivityFeeds">
    <div class="scrollbar">
        <div class="track">
            <div id="user-feeds-bar" class="thumb disable">
                <div class="end">
                </div>
            </div>
        </div>
    </div>
    <div class="viewport">
        <div id="userFeedsHolder" class="overview">
            <div id="loader-UserActivityFeeds">
                @Html.ImageBuilder(Url, "ajax-loader.gif", new { width = 50, height = 50 })
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        var userParams = { userId: '@ViewBag.viewedUserUserId' };
        //loadUserFeeds(userParams);
        $("#profileTabButton").live("click", function () {
            loadUserFeeds(userParams);
        });
    });
</script>
